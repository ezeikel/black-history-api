datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
  binaryTargets   = ["native", "rhel-openssl-1.0.x"]
}

model Person {
  id        String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  firstName String
  lastName  String
}
