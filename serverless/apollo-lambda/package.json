{
  "name": "black-history-api",
  "description": "",
  "author": "hi@ezeikel.dev",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn prisma:generate && sls offline --aws-profile ezeikel-admin",
    "lint": "jest --config test/jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|css|html|md)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "deploy": "NODE_ENV=production sls deploy --aws-profile ezeikel-admin",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prepare": "cd ../.. && husky install serverless/apollo-lambda/.husky"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "**/*.+(ts|js|json|md)": [
      "prettier --write",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@prisma/client": "^3.1.1",
    "@sentry/serverless": "^6.13.2",
    "apollo-server-lambda": "^3.3.0",
    "cloudinary": "^1.27.1",
    "express": "^4.17.1",
    "graphql": "^15.6.0",
    "graphql-upload": "^12.0.0",
    "prisma": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/graphql-upload": "^8.0.7",
    "@types/node": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "husky": "^7.0.2",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.2.1",
    "jest-runner-eslint": "^0.11.1",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "serverless-domain-manager": "^5.1.5",
    "serverless-offline": "^8.2.0",
    "serverless-webpack": "^5.5.4",
    "test-data-bot": "^0.8.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-node-externals": "^3.0.0"
  }
}
